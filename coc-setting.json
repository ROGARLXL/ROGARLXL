{
  "suggest.autoTrigger": "always",
  "highlight.document.enable": true,
  "highlight.colors.enable": true,
  "highlight.disableLanguages": ["python"],
  "clangd.enabled": true,
  "suggest.completionItemKindLabels": {
    "class": "\uf0e8",
    "color": "\ue22b",
    "constant": "\uf8fe",
    "default": "\uf29c",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "event": "\ufacd",
    "field": "\uf93d",
    "file": "\uf723",
    "folder": "\uf115",
    "function": "\u0192",
    "interface": "\uf417",
    "keyword": "\uf1de",
    "method": "\uf6a6",
    "module": "\uf40d",
    "operator": "\uf915",
    "property": "\ue624",
    "reference": "\ufa46",
    "snippet": "\ue60b",
    "struct": "\ufb44",
    "text": "\ue612",
    "typeParameter": "\uf728",
    "unit": "\uf475",
    "value": "\uf89f",
    "variable": "\ue71b"
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "html",
    "css",
    "json",
    "java",
    "python",
    "vue",
    "svelte",
    "c",
    "cpp",
    "cs",
    "racket",
    "prisma"
  ],
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"]
    }
    /*,
		"clangd": {
			"command": "clangd",
			"rootPatterns": [
				"compile_flags.txt",
				"compile_commands.json"
			],
			"filetypes": [
				"c",
				"cc",
				"cpp",
				"c++",
				"objc",
				"objcpp"
			]
		}*/
  },
  //explorer setting
  "explorer.git.enable": true,
  "explorer.icon.enableNerdfont": true,
  //coc-yank
  "yank.highlight.enable": true,
  "yank.highlight.duration": 300,
  //coc-markdownlint
  "markdownlint.config": {
    "default": true,
    "line_length": false
  },
  "markdownlint.onSave": true,
  //coc-explorer
  "explorer.icon.source": "vim-devicons"
}
